# 企业微信配置
CORP_ID=wwxxxxxxxxxxxxxxxx
AGENT_ID=1000001
SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TOKEN=your_wechat_token
ENCODING_AES_KEY=your_encoding_aes_key_43chars

# GitHub 配置
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_REPO=your_username/your_repo
GITHUB_BRANCH=main

# 服务配置
PORT=3000
FLASK_ENV=production

# 阿里云配置（用于显示）
DOCKER_REGISTRY=registry.cn-hangzhou.aliyuncs.com
DOCKER_NAMESPACE=your_namespace

# 青云对象存储配置（可选）
QINGSTOR_ACCESS_KEY_ID=your_qingstor_access_key_id
QINGSTOR_SECRET_ACCESS_KEY=your_qingstor_secret_access_key
QINGSTOR_ZONE=pek3a

# 代理配置（可选）
# HTTP 代理格式: http://proxy.example.com:8080
# SOCKS5 代理格式: socks5://proxy.example.com:1080
PROXY_URL=http://proxy.example.com:8080

# 不使用代理的域名列表（逗号分隔）
# 例如：github.com,qingstor.com
NO_PROXY_DOMAINS=localhost,127.0.0.1,qingstor.com

